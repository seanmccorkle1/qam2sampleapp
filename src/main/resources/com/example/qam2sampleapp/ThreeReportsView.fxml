            </columns>
            <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Label layoutX="6.4" layoutY="324.8" text="By month" AnchorPane.leftAnchor="6.4" AnchorPane.topAnchor="324.8">
            <font>
                <Font name="Calibri" size="17.0" />
            </font>
        </Label>
        <TableView fx:id="first_table" layoutX="3.0" layoutY="351.0" prefHeight="184.0" prefWidth="239.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="508.0" AnchorPane.topAnchor="351.0">
            <columns>
                <TableColumn fx:id="month_col" prefWidth="80.0" text="Month" />
                <TableColumn fx:id="appmt_type_column" prefWidth="54.399993896484375" text="Name" />
                <TableColumn fx:id="by_month_total" maxWidth="92.79998779296875" minWidth="65.5999755859375" prefWidth="92.79998779296875" text="Total" /> <!-- resizable="false"-->
            </columns>
            <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>

        <Label layoutX="547.0" layoutY="322.0" prefHeight="26.0" prefWidth="80.0" text="By year">
            <font>
                <Font name="Calibri" size="17.0" />
            </font>
        </Label>
        <TableView fx:id="second_table" layoutX="545.0" layoutY="351.0" prefHeight="184.0" prefWidth="246.0">
            <columns>
                <TableColumn fx:id="contact_year_col" prefWidth="85.0" text="Year" />
                <TableColumn fx:id="contact_name_col" prefWidth="89.60003662109375" text="Name" />
                <TableColumn fx:id="by_year_total" maxWidth="105.60003662109375" minWidth="81.0" prefWidth="83.99993896484375" text="Total" />
            </columns>
            <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
      <TableView fx:id="div_table" layoutX="288.0" layoutY="349.0" prefHeight="184.0" prefWidth="246.0">
          <columns>
              <TableColumn fx:id="div_col" prefWidth="100.0" text="State" />
              <TableColumn fx:id="name_col" prefWidth="110.0" text="Name" />
              <TableColumn fx:id="by_div_total" prefWidth="35.0" text="Total" />
          </columns>
      </TableView>
