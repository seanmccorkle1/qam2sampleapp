    <children>
        <Label layoutX="10.4" layoutY="2.2" text="Upcoming meetings" AnchorPane.leftAnchor="10.4" AnchorPane.topAnchor="2.2">
            <font><Font name="Calibri" size="18.0" /></font>
        </Label>
        <ComboBox fx:id="contact_dropdown" layoutX="11.2" layoutY="26.6" onAction="#show_appointments_for_a_contact" prefWidth="155.0" promptText="Pick a person" style="-fx-background-color:pink;" AnchorPane.leftAnchor="11.2" AnchorPane.topAnchor="26.6" />
        <TableView fx:id="main_table" layoutX="14.4" layoutY="59.2" prefHeight="211.0" prefWidth="484.0" AnchorPane.bottomAnchor="280.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="252.0" AnchorPane.topAnchor="59.0">
            <columns>
                <TableColumn fx:id="id_col" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" text="ID" /> <!-- resizable="false"-->
                <TableColumn fx:id="title_col" prefWidth="85.0" text="Title" />
                <TableColumn fx:id="type_col" prefWidth="66.4000015258789" text="Type" />
                <TableColumn fx:id="desc_col" prefWidth="114.39999389648438" text="Purpose" />
                <TableColumn fx:id="start_date_col" prefWidth="67.20001220703125" text="Start time" />
                <TableColumn fx:id="end_date_col" prefWidth="62.4000244140625" text="End time" />
