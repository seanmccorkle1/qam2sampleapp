<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0"
            prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.qam2sampleapp.LoginViewController">
    <children>
        <Text fx:id="header" strokeType="OUTSIDE" textAlignment="CENTER" AnchorPane.leftAnchor="25.0"
              AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="22.0">
            <font>
                <Font name="Calibri" size="22.5" />
            </font>
        </Text>
        <Label fx:id="time_zone" prefHeight="20.0" prefWidth="250.0" text="{{Timezone}}" AnchorPane.bottomAnchor="3.0"
               AnchorPane.rightAnchor="3.0" />
        <VBox alignment="CENTER" layoutX="15.0" prefHeight="550.0" prefWidth="320.0" spacing="15.0">
            <padding>
                <Insets bottom="40.0" left="25.0" right="45.0" top="25.0" />
            </padding>
            <children>
                <HBox prefHeight="32.0" prefWidth="236.0">
                    <children>
                        <Label fx:id="label" />
                    </children>
                </HBox>

                <TextField fx:id="username_input_field" promptText="Username..." style="-fx-background-color: #FAEBD7;">
                    <font>
                        <Font size="15.0" />
                    </font>
                </TextField>
                <PasswordField fx:id="password_input_field" promptText="Password..." style="-fx-background-color: #FAEBD7;">
                    <font>
                        <Font size="15.0" />
                    </font>
                </PasswordField>
                <Text fx:id="error_text" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="250.0">
                    <font>
                        <Font size="12.5" />
                    </font>
                </Text>
                <HBox alignment="CENTER" prefHeight="115.0" prefWidth="215.0" spacing="15.0">
                    <children>
                        <Button fx:id="login_button" onMouseClicked="#log_in" prefHeight="35.0" prefWidth="115.0"
                                style="-fx-background-color: #98FB98;">
                            <font>
                                <Font name="Calibri" size="15.0" />
                            </font>
                        </Button>
                        <Button fx:id="exit_button" mnemonicParsing="false" onMouseClicked="#exit_app" prefHeight="35.0"
                                prefWidth="115.0">
                            <font>
                                <Font name="Calibri" size="15.0" />
                            </font>
                        </Button>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="72.0" prefWidth="209.0">
                    <children>
                        <ImageView fx:id="flag" fitHeight="50.0" fitWidth="70.0" preserveRatio="true"
                                   AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
                            <image>
                                <Image url="@flags/ecuador.png" />
                            </image>
                        </ImageView>
                    </children>
                </HBox>

            </children>
        </VBox>
    </children>
</AnchorPane>

